@Library("shared-library") _
pipeline {
   agent any
      stages {
         stage('stage 1') {
            steps {
              helloworld()
            }
          }
          stage('stage 2') {
            steps{
                zip zipFile: 'hello.zip', glob: 'helloworld.html'
                archiveArtifacts artifacts: 'hello.zip', fingerprint: true
            }     
          }    
      }
}
