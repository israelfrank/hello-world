@Library("shared-library") _
pipeline {
   agent any
      stages {
         stage('stage 1') {
            steps {
              helloworld()
            }
          }
          stage('stage 2') {
            steps{
                script{
                    zip archive: true, dir: 'helloworld.html', glob: '', zipFile: 'hello.zip'
                } 
            }     
          }    
      }
}
